{
  "manifest_version": 3,
  "key": "",
  "name": "Maps Directions",
  "version": "1.0",
  "description": "Get directions to any location from selected text",
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Maps Directions"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "geolocation",
    "identity",
    "aiLanguageModelOriginTrial"
  ],
  "optional_host_permissions": [
    "<all_urls>"
  ],
  "oauth2": {
    "client_id": "",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "host_permissions": [
    "https://maps.googleapis.com/*"
  ],
  "background": {
    "service_worker": "gmaps/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "gmaps/content.js"
      ],
      "css": [
        "gmaps/content.css"
      ]
    }
  ],
  "options_page": "options/options.html",
  "web_accessible_resources": [
    {
      "resources": [
        "*.woff2",
        "*.ttf"
      ],
      "matches": [
        "<all_urls>"
      ],
      "use_dynamic_url": true
    }
  ]
}
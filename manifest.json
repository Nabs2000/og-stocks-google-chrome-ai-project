{
  "manifest_version": 3,
  "name": "OG stocks",
  "version": "1.0",
  "description": "Gemini Dev project",
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Click Me!"
  },
  "permissions": [
    "activeTab",
    "scripting",
    "contextMenus",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["scripts/content.js"]
    }
  ]
}
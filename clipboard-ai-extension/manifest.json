{
  "manifest_version": 3,
  "key": "",
  "name": "Clipboard AI",
  "version": "1.0",
  "description": "Your all-around Chrome extension for summarizing and getting directions.",
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "OG Stocks"
  },

  "permissions": [
    "activeTab",
    "scripting",
    "contextMenus",
    "storage",
    "geolocation",
    "identity",
    "aiLanguageModelOriginTrial",
    "sidePanel"
  ],

  "optional_host_permissions": ["<all_urls>"],

  "host_permissions": [
    "https://maps.googleapis.com/*",
    "https://generativelanguage.googleapis.com/*",
    "https://mail.google.com/*",
    "<all_urls>"
  ],

  "oauth2": {
    "client_id": "521658906047-6m7vvg2ntt1qg5ilnlv7csp8cveepdl1.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },

  "background": {
    "service_worker": "background.js"
  },

  "side_panel": {
    "default_path": "panel/sidepanel.html"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],

  "trial_tokens": [
    "AmYQqHjdkGY0Ke3xOns/0Szs+zYbBwWEgBzhfOP1dmfEIJjrMr8RNDy5GZOHsIIT7BvLHJYfl2xxY1uLMXcqzgQAAABseyJvcmlnaW4iOiJjaHJvbWUtZXh0ZW5zaW9uOi8vZWNqb2JuY2tuZWZsamdwaGxsZ2Nla25vbmZjY2NuaWoiLCJmZWF0dXJlIjoiQUlXcml0ZXJBUEkiLCJleHBpcnkiOjE3NzY3Mjk2MDB9"
  ],

  "web_accessible_resources": [
    {
      "resources": ["*.woff2", "*.ttf"],
      "matches": ["<all_urls>"],
      "use_dynamic_url": true
    }
  ]
}
